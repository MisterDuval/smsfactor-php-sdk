{
    "name": "smsfactor/smsfactor-php-sdk",
    "description": "SMSFactor client library for PHP",
    "keywords": [
        "sms", "api"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "SMSFactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMSFactor\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.0@dev"
    }
}
